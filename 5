Палиндромы

#include <iostream>
#include <string>
#include <algorithm>

int main() 
{
    std::string input;
    std::getline(std::cin, input);

    input.erase(remove(input.begin(), input.end(), ' '), input.end());

    std::string reversed_input = input;
    std::reverse(reversed_input.begin(), reversed_input.end());

    if (input == reversed_input) {
        std::cout << "YES";
    } else {
        std::cout << "NO";
    }

}
